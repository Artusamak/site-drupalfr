{
  "name": "Drupal-FR/site-drupalfr",
  "description": "drupal.fr repository.",
  "type": "project",
  "homepage": "http://drupal.fr",
  "license": "GPL-2.0+",
  "authors": [
    {
      "name": "Florent Torregrosa",
      "homepage": "http://drupal.fr/user/40695",
      "role": "Developer"
    }
  ],
  "support": {
    "issues": "https://github.com/Drupal-FR/site-drupalfr/issues",
    "source": "https://github.com/Drupal-FR/site-drupalfr",
    "docs": "https://github.com/Drupal-FR/site-drupalfr/blob/8.x-1.x/README.md"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packagist.drupal-composer.org"
    }
  ],
  "require": {
    "composer/installers": "^1.0.21",
    "cweagans/composer-patches": "~1.0",
    "drupal/core": "8.1.8",
    "drush/drush": "8.1.3",
    "drupal/admin_toolbar": "8.1.16",
    "drupal/eu_cookie_compliance": "8.1.0-beta5",
    "drupal/features": "8.3.0-beta7",
    "drupal/honeypot": "8.1.23",
    "drupal/pathauto": "8.1.0-alpha3",
    "drupal/r4032login": "8.1.*",
    "drupal/redis": "8.1.0-alpha1"
  },
  "require-dev": {
    "drupal/devel": "8.1.*",
    "drupal/devel_a11y": "8.1.*",
    "drupal/config_inspector": "8.1.*",
    "drupal/examples": "8.1.*",
    "drupal/pistachio": "8.1.*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "post-install-cmd": [
      "bash ../scripts/update-scaffold.sh",
      "bash ../scripts/update-libraries.sh"
    ]
  },
  "extra": {
    "installer-paths": {
      "core": ["type:drupal-core"],
      "modules/contrib/{$name}": ["type:drupal-module"],
      "profiles/contrib/{$name}": ["type:drupal-profile"],
      "themes/contrib/{$name}": ["type:drupal-theme"],
      "libraries/{$name}": ["type:drupal-library"]
    },
    "patches": {
      "drupal/features": {
        "Not detecting overrides in profile config files": "https://www.drupal.org/files/issues/features-not_detecting_profile_files-2668562-2.patch",
        "Drush export features do not overwrite the existing feature": "https://www.drupal.org/files/issues/features-drush_export_overwrite-2672406-5.patch",
        "Warning uasort in Features UI": "https://www.drupal.org/files/issues/features-warning_uasort-2672470-10.patch"
      },
      "drupal/redis": {
        "Currently Drush Cr or Cache Clear UI does not flush Redis cache": "https://www.drupal.org/files/issues/redis-drush_cr-2765895-4.patch"
      }
    }
  }
}
