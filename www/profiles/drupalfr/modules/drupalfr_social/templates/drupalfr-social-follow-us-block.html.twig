{#
/**
 * @file
 * Default theme implementation to display follow us block.
 *
 * Available variables:
 * - url_facebook
 * - url_twitter
 * - url_linkedin
 * - url_viadeo
 * - url_youtube
 *
 * @ingroup themeable
 */
#}
<ul>
  {% if url_facebook %}
    <li>
      <a href="{{ url_facebook }}" title="{{ 'Facebook'|t }}">{{ 'Facebook'|t }}</a>
    </li>
  {% endif %}
  {% if url_twitter %}
    <li>
      <a href="{{ url_twitter }}" title="{{ 'Twitter'|t }}">{{ 'Twitter'|t }}</a>
    </li>
  {% endif %}
  {% if url_linkedin %}
    <li>
      <a href="{{ url_linkedin }}" title="{{ 'LinkedIn'|t }}">{{ 'LinkedIn'|t }}</a>
    </li>
  {% endif %}
  {% if url_viadeo %}
    <li>
      <a href="{{ url_viadeo }}" title="{{ 'Viadeo'|t }}">{{ 'Viadeo'|t }}</a>
    </li>
  {% endif %}
  {% if url_youtube %}
    <li>
      <a href="{{ url_youtube }}" title="{{ 'Youtube'|t }}">{{ 'Youtube'|t }}</a>
    </li>
  {% endif %}
</ul>
