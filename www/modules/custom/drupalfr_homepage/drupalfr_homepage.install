<?php

/**
 * @file
 * Install, update and uninstall functions for the drupalfr_homepage module.
 */

/**
 * Implements hook_install().
 */
function drupalfr_homepage_install() {
  // Set front page to "home".
  \Drupal::configFactory()
    ->getEditable('system.site')
    ->set('page.front', '/home')
    ->save(TRUE);
}
