{
  "name": "Drupal-FR/site-drupalfr",
  "description": "drupal.fr repository.",
  "type": "project",
  "homepage": "https://drupal.fr",
  "license": "GPL-2.0+",
  "authors": [
    {
      "name": "Florent Torregrosa",
      "homepage": "https://drupal.fr/user/40695",
      "role": "Developer"
    }
  ],
  "support": {
    "issues": "https://github.com/Drupal-FR/site-drupalfr/issues",
    "source": "https://github.com/Drupal-FR/site-drupalfr",
    "docs": "https://github.com/Drupal-FR/site-drupalfr/blob/8.x-1.x/README.md"
  },
  "repositories": {
    "drupal": {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    "leafletjs": {
      "type": "package",
      "package": {
        "name": "leaflet/leaflet",
        "version": "v0.7.7",
        "type": "drupal-library",
        "dist": {
          "url": "https://github.com/Leaflet/Leaflet/archive/v0.7.7.zip",
          "type": "zip"
        }
      }
    },
    "ows_slideshare": {
      "type": "vcs",
      "url": "git@github.com:OWS/ZendService_SlideShare.git"
    },
    "dropzone": {
      "type": "package",
      "package": {
        "name": "dropzone/dropzone",
        "version": "v4.3.0",
        "type": "drupal-library",
        "source": {
          "url": "https://github.com/enyo/dropzone",
          "type": "git",
          "reference": "v4.3.0"
        }
      }
    },
    "d3": {
      "type": "package",
      "package": {
        "name": "d3/d3",
        "version": "v3.5.17",
        "type": "drupal-library",
        "source": {
          "url": "https://github.com/d3/d3",
          "type": "git",
          "reference": "v3.5.17"
        }
      }
    },
    "highlightjs": {
      "type": "package",
      "package": {
        "name": "components/highlightjs",
        "version": "9.9.0",
        "type": "drupal-library",
        "source": {
          "url": "https://github.com/components/highlightjs",
          "type": "git",
          "reference": "9.9.0"
        }
      }
    }
  },
  "require": {
    "composer/installers": "^1.0.21",
    "cweagans/composer-patches": "~1.0",
    "drupal-composer/drupal-scaffold": "^2.2",
    "drupal/core": "8.3.0-beta1",
    "drush/drush": "8.1.10",
    "drupal/drush_language": "1.*",
    "drupal/admin_toolbar": "1.18.0",
    "drupal/dropzonejs": "1.0.0-alpha5",
    "drupal/entity_browser": "1.0.0-rc2",
    "drupal/entity_embed": "1.0.0-beta2",
    "drupal/eu_cookie_compliance": "1.0.0-beta7",
    "drupal/facets": "1.0.0-alpha7",
    "drupal/features": "3.3.0",
    "drupal/feeds": "3.*",
    "drupal/geofield": "1.0.0-alpha2",
    "drupal/honeypot": "1.23.0",
    "drupal/inline_entity_form": "1.0.0-beta1",
    "drupal/leaflet": "1.0.0-beta1",
    "drupal/mailchimp": "1.2.0",
    "drupal/media_entity": "1.6.0",
    "drupal/media_entity_document": "1.1.0",
    "drupal/media_entity_image": "1.2.0",
    "drupal/media_entity_slideshare": "1.3.0",
    "drupal/migrate_plus": "3.0.0-beta1",
    "drupal/migrate_source_csv": "2.0.0",
    "drupal/migrate_tools": "3.0.0-beta1",
    "drupal/pathauto": "1.0.0-rc1",
    "drupal/purge": "3.0.0-beta6",
    "drupal/r4032login": "1.*",
    "drupal/redis": "1.0.0-beta1",
    "drupal/search_api": "1.0.0-beta4",
    "drupal/search_api_solr": "1.0.0-beta1",
    "drupal/varnish_purge": "1.4.0",
    "drupal/video_embed_field": "1.4.0",
    "abraham/twitteroauth": "0.7.2",
    "dropzone/dropzone": "4.3.*",
    "drupal/bootstrap": "^3.2"
  },
  "require-dev": {
    "drupal/console": "1.*",
    "drupal/config_inspector": "1.*",
    "drupal/devel": "1.*",
    "drupal/devel_a11y": "1.*",
    "drupal/examples": "1.*",
    "drupal/renderviz": "1.*",
    "drupal/search_kint": "1.*",
    "drupal/pistachio": "1.*",
    "d3/d3": "3.5.*",
    "components/highlightjs": "9.9.*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true
  },
  "scripts": {
    "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
    "post-create-project-cmd": [
      "bash ./scripts/init.sh"
    ],
    "post-install-cmd": [
      "@drupal-scaffold",
      "bash ./scripts/update-libraries.sh"
    ],
    "post-update-cmd": [
      "@drupal-scaffold",
      "bash ./scripts/update-libraries.sh"
    ]
  },
  "extra": {
    "installer-paths": {
      "www/core": ["type:drupal-core"],
      "www/modules/contrib/{$name}": ["type:drupal-module"],
      "www/modules/custom/{$name}": ["type:drupal-custom-module"],
      "www/themes/contrib/{$name}": ["type:drupal-theme"],
      "www/themes/custom/{$name}": ["type:drupal-custom-theme"],
      "www/profiles/contrib/{$name}": ["type:drupal-profile"],
      "www/libraries/{$name}": ["type:drupal-library"],
      "drush/contrib/{$name}": ["type:drupal-drush"]
    },
    "drupal-scaffold": {
      "excludes": [
        "update.php",
        "web.config",
        "sites/development.services.yml",
        "sites/example.settings.local.php"
      ]
    },
    "patches": {
      "drupal/entity_browser": {
        "Fixed the issue of Call to a member function getConfigDependencyKey() on null on [Widget view], and [SelectionDisplay view]": "https://www.drupal.org/files/issues/2845037-4.patch"
      },
      "drupal/leaflet": {
        "Error in leaflet_process_geofield() when twig debug is active.": "https://www.drupal.org/files/issues/leaflet-error_when_debug_active-2783843-2.patch"
      },
      "drupal/redis": {
        "Currently Drush Cr or Cache Clear UI does not flush Redis cache": "https://www.drupal.org/files/issues/redis-drush_cr-2765895-4.patch"
      }
    }
  }
}
